{
  "name": "leetcode-solutions",
  "version": "1.0.0",
  "description": "A comprehensive collection of LeetCode problem solutions with detailed test cases and performance analysis",
  "main": "index.js",
  "scripts": {
    "test": "node scripts/test.js",
    "test:debug": "node --inspect-brk scripts/test.js",
    "debug": "node --inspect-brk scripts/test.js",
    "debug:terminal": "node inspect scripts/test.js",
    "test:all": "node scripts/test.js all",
    "test:performance": "node scripts/test.js",
    "test:verbose": "node scripts/test.js",
    "test:detail": "node scripts/test.js",
    "test:filter": "node scripts/test.js",
    "test:category": "node scripts/test.js",
    "test:output": "node scripts/test.js",
    "create": "node scripts/create-solution.js",
    "create:insights": "node scripts/create-insights.js",
    "create:full": "node scripts/create-solution.js -- --with-insights",
    "lint": "eslint src scripts --ext .js",
    "format": "prettier --write \"src/**/*.js\" \"scripts/**/*.js\"",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run format"
  },
  "keywords": [
    "leetcode",
    "algorithms",
    "data-structures",
    "testing",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4"
  }
}
